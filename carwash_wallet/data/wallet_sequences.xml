<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Sequence for Wallet Top-up Reference Numbers -->
    <record id="seq_wallet_topup" model="ir.sequence">
        <field name="name">Wallet Top-up Sequence</field>
        <field name="code">wallet.topup</field>
        <field name="prefix">WTU/</field>
        <field name="padding">5</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
    </record>
</odoo>
